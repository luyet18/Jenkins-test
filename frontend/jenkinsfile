pipeline {
  agent any

  tools { 
    nodejs "node" // node: nombre definido en Global Tool Configuration (instalaciones de NodeJS)
  }

  stages {
    stage('install') {
      steps {
        git branch: 'master', url: 'https://github.com/luyet18/Jenkins-test.git'
        dir('frontend') {
         bat 'npm install'
        }
      }
    }

  }

}